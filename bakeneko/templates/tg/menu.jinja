{% extends "base.jinja" %}
{% block title %}
    TG Web Menu
{% endblock title %}
{% block body %}
    <form>
        <input type="text" id="q" name="q" value="q text"/>
        <button id="search" hx-post="/tg-web-app/search/" hx-target="#result" hx-vals='js:{init: window.Telegram.WebApp.initData}'>?</button>
    </form>
    <div id="result"></div>
{% endblock body %}
